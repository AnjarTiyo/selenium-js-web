{
  "name": "selenium-js-web",
  "version": "1.0.0",
  "description": "Web Automation Testing",
  "main": "index.js",
  "scripts": {
    "test": "cucumber-js --retry 1 --format json:./reports/report.json",
    "test:register-corp": "cucumber-js --tags '@Register and @Corp' ",
    "test:register-user": "cucumber-js --tags '@Register and @User' ",
    "test:forgot": "cucumber-js --tags @ForgotPassword",
    "debug": "cucumber-js --tags @Only"
  },
  "keywords": [
    "selenium",
    "web-automation"
  ],
  "author": "Anjar Tiyo Saputro",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "browsermob-proxy": "^1.0.13",
    "chai": "^4.3.8",
    "chromedriver": "^116.0.0",
    "cucumber": "^6.0.7",
    "cucumber-html-reporter": "^7.1.1",
    "dotenv": "^16.3.1",
    "selenium-webdriver": "^4.12.0"
  }
}
